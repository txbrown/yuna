{
  "name": "yuna",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "pods": "cd ios && pod install"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@shopify/react-native-skia)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleNameMapper": {
      "^expo-sqlite$": "<rootDir>/features/notes/__mocks__/expo-sqlite.ts",
      "^expo-crypto$": "<rootDir>/features/notes/__mocks__/expo-crypto.ts"
    }
  },
  "dependencies": {
    "@expo-google-fonts/orbitron": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/native": "^7.1.8",
    "@shopify/react-native-skia": "^2.4.6",
    "@tanstack/react-query": "^5.90.11",
    "cactus-react-native": "^1.2.0",
    "expo": "~54.0.25",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-font": "~14.0.9",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-sqlite": "^16.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@react-native-community/cli": "^20.0.2",
    "@react-native-community/cli-platform-android": "^20.0.2",
    "@react-native-community/cli-platform-ios": "^20.0.2",
    "@react-native/metro-config": "^0.82.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.13",
    "metro-config": "^0.83.3",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
